  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "yarn lint && yarn type-check && yarn build",
    "eject": "react-scripts eject",
    "lint": "yarn lint:js && yarn lint:css",
    "lint:js": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:css": "stylelint src/*.{css,scss,sass}",
    "type-check": "tsc",
    "watch:build": "watch \"yarn build\" src"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@craco/craco": "^6.2.0",
    "@hookform/resolvers": "^2.6.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.11.1",
    "react-icons": "^4.2.0",
    "react-scripts": "4.0.3",
    "zod": "^3.5.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/chrome": "^0.0.147",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "autoprefixer": "^9",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-tailwind": "^0.2.1",
    "postcss": "^7",
    "prettier": "^2.3.2",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended": "^5.0.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "typescript": "^4.1.2"
  }
}
