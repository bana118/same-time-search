{
  "short_name": "sts",
  "name": "Same Time Search",
  "permissions": ["tabs", "storage"],
  "manifest_version": 2,
  "version": "0.0.2",
  "browser_action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["static/js/content.js"]
    }
  ],
  "options_page": "options.html"
}
